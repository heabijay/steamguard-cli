<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>SteamGuard</RootNamespace>
    <Description>Fork of https://github.com/dyc3/steamguard-cli</Description>
    <Authors>heabijay &amp; dyc3</Authors>
